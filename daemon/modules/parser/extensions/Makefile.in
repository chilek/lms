CFLAGS+=-I../lib -D_GNU_SOURCE
OBJECTS+=../../../dbdrivers/$(DBDRIVER)/db.o ../../../util.o \
	tscript_exec.o tscript_string.o sql.o consts.o net.o
HEADERS=tscript_exec.h tscript_string.h sql.h consts.h net.h

extensions.a: $(OBJECTS)
	ar rs tscript_extensions.a $(OBJECTS)

$(OBJECTS): %.o: %.c $(HEADERS)
	gcc -c $< $(CFLAGS)

clean:
	rm -f tscript_extensions.a *.o

		FOREIGN KEY (nodeid) REFERENCES nodes (id) ON DELETE SET NULL ON UPDATE CASCADE
) ENGINE=InnoDB;

# --------------------------------------------------------
#
# Structure of table stats
#
CREATE TABLE stats (
	nodeid int(11)		DEFAULT NULL,
